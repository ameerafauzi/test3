</script>  
{% endcomment %}  
  
<script type='application/ld+json'> 
{
	"@context": "http://schema.org",
	"@type": "LocalBusiness",
	"logo": "https://cdn.shopify.com/s/files/1/2155/6875/t/25/assets/logo.png?3787550150927998551",
	"image": "https://lh5.googleusercontent.com/p/AF1QipOGxtN9l7NRCrw9YjgGmMyUpjThqxmbTRNicJxH=w408-h229-k-no",
	"name": "{{ shop.name }}",
	"telephone": "03-7875 3692",
	"geo":{
		"@type": "GeoCoordinates",
		"latitude": "3.112978",
		"longitude": "101.6202579"
	},
	"url": "{{ shop.url }}",
	"priceRange": "$$$",
	"email": "{{ shop.email }}",
	"contactPoint":	{
		"@type": "ContactPoint",
		"telephone": "+60378753692",
		"contactType": "Technical Support",
		"email": "esh2uonline@gmail.com"
	},
	"address":	{
		"@type": "PostalAddress",
		"addressCountry": "{{ shop.address.country }}",
		"addressLocality": "{{ shop.address.city }}",
		"addressRegion": "{{ shop.address.province }}",
		"postalCode": "{{ shop.address.zip }}",
		"streetAddress": "{{ shop.address.street }}"
	},
	"openingHours": "Mo-Sa 10:00-21:00",
	"sameAs": ["https://m.facebook.com/esh2u.MY"]
} 
</script>
  
<script type='application/ld+json'>
{
  "@context": "http://schema.org",
  "@type": "WebSite",
  "url": "{{ shop.url }}",
  "potentialAction": {"@type": "SearchAction",
  "target": "{{ shop.url }}/search?q={query}&category_id=product&type=product",
  "query-input": "required name=query"}
}
</script>
  
{% if template == "product" %} 
<script type='application/ld+json'>
{
	"@context": "http://schema.org/",
	"@type": "Product",
	"name": "{{ product.title }}",
	"image": "{{ product.featured_image }}",
	"description": "{{ product.description | strip_html | truncatewords: 20 }}",    
	"brand":
	{
		"@type": "Thing",
		"name": "{{ product.vendor }}"
	},
	"offers":
	{
		"@type": "Offer",
		"priceCurrency": "{{ shop.currency }}",
		"price": "{{ product.price | divided_by: 100 }}",
		
		
		{% if product.available %}
		  "availability": "http://schema.org/InStock"
		{% else %}
		  "availability": "http://schema.org/SoldOut"
		{% endif %}
	}
} 
</script>
{% endif %}  
  
</head>
{% include 'modules-init' %}
{% capture body_class %}{% if settings.page_width == '3'  %} body-full-width{% endif %}{% if settings.advanced_product_list != '' %} product-list-type-{{settings.advanced_product_list}}{% endif %}{% if settings.advanced_product_grid != '' %} product-grid-type-{{settings.advanced_product_grid}}{% endif %}{% if settings.advanced_dropdown_menu != '' %} dropdown-menu-type-{{settings.advanced_dropdown_menu}}{% endif %}{% if settings.advanced_product_button != '' %} products-buttons-action-type-{{settings.advanced_product_button}}{% endif %}{% if settings.advanced_prev_next != '' %} buttons-prev-next-type-{{settings.advanced_prev_next}}{% endif %}{% if settings.advanced_input != '' %} inputs-type-{{settings.advanced_input}}{% endif %}{% if settings.advanced_myaccount != '' %} my-account-type-{{settings.advanced_myaccount}}{% endif %}{% if settings.advanced_cart_block != '' %} cart-block-type-{{settings.advanced_cart_block}}{% endif %}{% if settings.advanced_top_bar != '' %} top-bar-type-{{settings.advanced_top_bar}}{% endif %}{% if settings.advanced_product_page != '' %} product-page-type-{{settings.advanced_product_page}}{% endif %}{% if settings.advanced_megamenu_type != '' %} megamenu-type-{{settings.advanced_megamenu_type}}{% endif %}{% if settings.advanced_search != '' %} search-type-{{settings.advanced_search}}{% endif %}{% if settings.advanced_megamenu_label != '' %} megamenu-label-type-{{settings.advanced_megamenu_label}}{% endif %}{% if settings.advanced_box == '7' %} box-type-4{% else %} no-box-type-7{% endif %}{% if settings.advanced_box != '' %} box-type-{{settings.advanced_box}}{% endif %}{% if settings.advanced_header_margin_top != '' %} header-margin-top-{{settings.advanced_header_margin_top}}{% endif %}{% if settings.advanced_sale_new != '' %} sale-new-type-{{settings.advanced_sale_new}}{% endif %}{% if settings.advanced_button != '' %} button-body-type-{{settings.advanced_button}}{% endif %}{% if settings.advanced_countdown != '' %} countdown-special-type-{{settings.advanced_countdown}}{% endif %}{% if settings.advanced_footer != '' %} footer-type-{{settings.advanced_footer}}{% endif %}{% if settings.advanced_breadcrumb != '' %} breadcrumb-type-{{settings.advanced_breadcrumb}}{% endif %}{% if settings.advanced_border_width != '' %} border-width-{{settings.advanced_border_width}}{% endif %}{% if settings.body_background_color == '#ffffff' or settings.main_background_color == settings.body_background_color or settings.main_background_color == 'rgba(0,0,0,0)' %} body-white{% else %} body-other{% endif %}{% if settings.main_background_color == 'rgba(0,0,0,0)' %} body-white-type-2{% endif %}{% if settings.main_background_color == 'rgba(0,0,0,0)' and settings.box_product_bg == '#ffffff' %} body-white-type-3{% endif %}{% if settings.advanced_banner_hover != '' %} banners-effect-{{settings.advanced_banner_hover}}{% endif %}{{' body-header-type-' | append: settings.header_type}}{% if settings.header_always_show_vermenu_cat %} show-vertical-megamenu-category-page{% endif %}{% if settings.header_always_show_vermenu %} show-vertical-megamenu{% endif %}{% endcapture %}
<body class="{% if template == 'index' %}common-home {% endif %}template{{ template | split: '.' | first | camelize }}{% if template == 'page.faq' %} module-faq{% endif %}{% if template == 'collection' or template contains 'collection.' %}{% if settings.collection_grid_list == 'list' %} cat-list{% endif %}{% endif %}{{body_class}}">
  {%- capture container_class -%}{% if settings.main_layout == '1' or settings.main_layout == '5' %}{{'standard-body'}}{% else %}{{'fixed-body'}}{% endif %}{% if settings.main_layout == '7' %}{{' fixed-body-shoes'}}{% endif %}{% if settings.main_layout == '4' or settings.main_layout == '6' %}{{' fixed-body-2'}}{% endif %}{% if settings.main_layout == '5' %}{{' fixed-body-2-2'}}{% endif %}{% if settings.main_layout == '3' %}{{' with-shadow'}}{% endif %}{%- endcapture -%}
  {%- capture main_class -%}{% if settings.main_layout == '4' %}{{'main-fixed2 main-fixed'}}{% elsif settings.main_layout == '6' %}{{'main-fixed2 main-fixed3 main-fixed'}}{% elsif settings.main_layout != '1' and settings.main_layout != '5' %}{{'main-fixed'}}{% endif %}{%- endcapture -%}
  {%- capture content_class -%}{% if settings.content_layout == '1' %}{{'full-width'}}{% elsif settings.content_layout == '4' %}{{'fixed2 fixed3'}}{% elsif settings.content_layout == '3' %}{{'fixed2'}}{% else %}{{'fixed'}}{% endif %}{%- endcapture -%}
  {% include 'header-notice' %}
  {% include 'top-promo' %}
  <div class="{{container_class}}">
    <div id="main" class="{{main_class}}">
      {%- comment -%} Header {%- endcomment -%}
      {% include 'header' %}
      
      {%- comment -%} Breadcrumb {%- endcomment -%}
      {%- if template != 'index' -%}
      {% include 'breadcrumb' %}
      {%- endif -%}
      
      {%- comment -%} Main Content {%- endcomment -%}
      <div class="main-content {{content_class}}{% if template == 'index' %} home{% endif %}">
        <div class="background-content"></div>
        <div class="background">
          <div class="shadow"></div>
          <div class="pattern">
            <div class="container">
              <div class="row">
                {%- if template == 'index' -%}
                <div class="col-md-12">
                  <div class="row">
                    {{content_for_layout}}
                  </div>
                </div>
                {%- else -%}
                <div class="col-md-12">
                  <div class="row">
                    {{content_for_layout}}
                  </div>
                  {%- if template == 'product' -%}
                  {% include 'product-bottom' %}
                  {%- endif -%}
                </div>
                {%- endif -%}
              </div>
            </div>    
          </div>
        </div>
      </div>
      {%- comment -%} Footer {%- endcomment -%}
      {%- comment -%}{% include 'footer-custom' %}{%- endcomment-%}
      {%- section "preface-footer" -%}
      {% include 'footer' %}
    </div>
    {%- comment -%} Widgets {%- endcomment -%}
    {% include 'social-widget' %}
  </div>
  {% include 'linked-options' %}
  {% include 'extension-notify' %}
  {% include 'extension-mailchimp' %}
  {% include 'extension-cookie' %}
  {% include 'notify-template' %}
  {%- if template == 'page.contact2' -%}{{ '//maps.google.com/maps/api/js?sensor=false' | script_tag }}{{ 'jquery.gmap.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'blog.grid' or template == 'blog.grid_3_columns' -%}{{ 'masonry.pkgd.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'collection' or template contains 'collection.' -%}{{ 'jquery.history.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'product' and settings.product_image_zoom != '2' -%}{{ 'jquery.elevateZoom.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'product' and settings.product_bottom_viewed -%}{{ 'jquery.products.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if settings.show_multiple_currencies -%}{{ '//cdn.shopify.com/s/javascripts/currencies.js' | script_tag }}{%- endif -%}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}
  {% include 'api.jquery.custom' %}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {{ 'lodash.min.js' | asset_url | script_tag }}
  {{ 'rt.application.js' | asset_url | script_tag }}

{% include 'special-offers' %}

